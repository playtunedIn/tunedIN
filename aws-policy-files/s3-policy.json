{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "S3Access",
			"Effect": "Allow",
			"Action": [
			        "s3:PutObject",
			        "s3:GetObject",
			        "s3:ListBucket"
			    ],
			"Resource": [
			        "arn:aws:s3:::tunedin-terraform-state-files/*",
			        "arn:aws:s3:::tunedin-terraform-state-files"
			    ]
		}
	]
}