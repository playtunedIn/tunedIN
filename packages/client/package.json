{
  "name": "client",
  "description": "Frontend for SpotiSpy.",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode=dev --host=local.playtunedin-test.com",
    "build": "vite build --mode=prod",
    "start": "vite preview --mode=prod --host=local.playtunedin-test.com",
    "______TESTING_________________": "",
    "test": "vitest run --coverage=false",
    "test:coverage": "vitest run --coverage=true",
    "______LINTING_________________": "",
    "lint": "pnpm run lint:css && pnpm run lint:js && pnpm run lint:types",
    "lint:css": "stylelint --max-warnings=0 'src/**/*.{css,scss,less}'",
    "lint:js": "eslint . --max-warnings=0",
    "lint:types": "tsc --noEmit",
    "______LIFECYCLE COMMANDS______": "",
    "generate-cert": "../../scripts/generateDevSSLCert.sh",
    "git:pre-push": "pnpm run test"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^0.33.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jsdom": "^22.1.0",
    "stylelint": "^15.9.0",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-recess-order": "^4.2.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "license": "UNLICENSED"
}
